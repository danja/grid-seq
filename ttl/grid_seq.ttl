@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://lv2plug.in/ns/ext/time#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<http://github.com/danny/grid-seq>
    a lv2:Plugin ,
        lv2:InstrumentPlugin ;
    doap:name "Grid Sequencer" ;
    doap:license <https://opensource.org/licenses/ISC> ;
    lv2:project <http://github.com/danny/grid-seq> ;
    lv2:requiredFeature urid:map ;
    lv2:optionalFeature lv2:hardRTCapable ;
    ui:ui <http://github.com/danny/grid-seq#ui> ;
    lv2:port [
        a lv2:InputPort ,
            atom:AtomPort ;
        atom:bufferType atom:Sequence ;
        atom:supports midi:MidiEvent ,
                      time:Position ;
        lv2:index 0 ;
        lv2:symbol "midi_in" ;
        lv2:name "MIDI In" ;
        lv2:designation lv2:control
    ] , [
        a lv2:OutputPort ,
            atom:AtomPort ;
        atom:bufferType atom:Sequence ;
        atom:supports midi:MidiEvent ;
        lv2:index 1 ;
        lv2:symbol "midi_out" ;
        lv2:name "MIDI Out (Notes)"
    ] , [
        a lv2:OutputPort ,
            atom:AtomPort ;
        atom:bufferType atom:Sequence ;
        atom:supports midi:MidiEvent ;
        lv2:index 2 ;
        lv2:symbol "launchpad_out" ;
        lv2:name "Launchpad Control"
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "grid_x" ;
        lv2:name "Grid X" ;
        lv2:default -1 ;
        lv2:minimum -1 ;
        lv2:maximum 7
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "grid_y" ;
        lv2:name "Grid Y" ;
        lv2:default -1 ;
        lv2:minimum -1 ;
        lv2:maximum 7
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "current_step" ;
        lv2:name "Current Step" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 7
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "grid_changed" ;
        lv2:name "Grid Changed Counter" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 999999
    ] , [
        a lv2:OutputPort ,
            atom:AtomPort ;
        atom:bufferType atom:Sequence ;
        lv2:index 7 ;
        lv2:symbol "notify" ;
        lv2:name "UI Notifications" ;
        lv2:designation lv2:control
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "grid_row_0" ;
        lv2:name "Grid Row 0" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "grid_row_1" ;
        lv2:name "Grid Row 1" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "grid_row_2" ;
        lv2:name "Grid Row 2" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 11 ;
        lv2:symbol "grid_row_3" ;
        lv2:name "Grid Row 3" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 12 ;
        lv2:symbol "grid_row_4" ;
        lv2:name "Grid Row 4" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 13 ;
        lv2:symbol "grid_row_5" ;
        lv2:name "Grid Row 5" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 14 ;
        lv2:symbol "grid_row_6" ;
        lv2:name "Grid Row 6" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 15 ;
        lv2:symbol "grid_row_7" ;
        lv2:name "Grid Row 7" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 16 ;
        lv2:symbol "grid_row_8" ;
        lv2:name "Grid Row 8" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 17 ;
        lv2:symbol "grid_row_9" ;
        lv2:name "Grid Row 9" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 18 ;
        lv2:symbol "grid_row_10" ;
        lv2:name "Grid Row 10" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 19 ;
        lv2:symbol "grid_row_11" ;
        lv2:name "Grid Row 11" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 20 ;
        lv2:symbol "grid_row_12" ;
        lv2:name "Grid Row 12" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 21 ;
        lv2:symbol "grid_row_13" ;
        lv2:name "Grid Row 13" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 22 ;
        lv2:symbol "grid_row_14" ;
        lv2:name "Grid Row 14" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 23 ;
        lv2:symbol "grid_row_15" ;
        lv2:name "Grid Row 15" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 255
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 24 ;
        lv2:symbol "sequence_length" ;
        lv2:name "Sequence Length" ;
        lv2:default 8 ;
        lv2:minimum 2 ;
        lv2:maximum 16 ;
        lv2:portProperty lv2:integer
    ] .

<http://github.com/danny/grid-seq#ui>
    a ui:GtkUI ;
    ui:binary <grid_seq_ui.so> ;
    lv2:requiredFeature ui:idleInterface ;
    lv2:extensionData ui:idleInterface .
